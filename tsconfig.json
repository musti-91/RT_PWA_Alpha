{
  "compilerOptions": {
    "target": "es2015",
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "rules":{
    "member-access":false,
  },
  "include": [
    "src"
  ],
  "lib": [
    "dom",
    "es5",
    "scripthost",
    "es2015.promise"
],
}
